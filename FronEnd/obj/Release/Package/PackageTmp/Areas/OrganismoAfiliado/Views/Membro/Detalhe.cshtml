    $("#DataInscricaoOA").datepicker({
        format: 'dd/mm/yyyy',
        language: 'pt-BR'
    });

    $("#DataAdmissaoOA").datepicker({
        format: 'dd/mm/yyyy',
        language: 'pt-BR'
    });

    $("#MembroCFD_Data").datepicker({
        format: 'dd/mm/yyyy',
        language: 'pt-BR'
    });

    $('#DataInscricaoOA').mask('99/99/9999');
    $('#DataAdmissaoOA').mask('99/99/9999');
    $('#MembroCFD_Data').mask('99/99/9999');

    $('a[data-toggle="tab"]').on('shown.bs.tab', function (e) {
        var target = $(e.target).attr("href").replace("#", "") // activated tab
        switch(target) {
            case "SituacaoFinanceira" :
                AbrirDetalhesMembroFinanceiro(tempModel);
                break;
            case "Iniciacoes" :
                AbrirDetalhesMembroIniciacoes(tempModel);
                break;
            case "HistoricoCompras" :
                AbrirDetalhesMembroHistoricoCompras(tempModel);
                break;
            case "HistoricoContribuicoes" :
                AbrirDetalhesMembroHistoricoContribuicoes(tempModel);
                break;
            case "HistoricoAtividades" :
                AbrirDetalhesMembroHistoricoAtividades(tempModel);
                break;
            case "HistoricoBiblioteca" :
                AbrirDetalhesMembroHistoricoBiblioteca(tempModel);
                break;
        }

    });

    function HabilitaMudancaTipoAfiliacao() {
        $("#pnlMudancaTipoAfiliacao").show();
    }

    function CloseMudancaTipoAfiliacao() {
        $("#pnlMudancaTipoAfiliacao").hide();
    }

    function ConfirmarMudancaTipoAfiliacao() {
        var AfiliacaoNumeroAtual = '@Model.AfiliacaoNumero';
        var AfiliacaoNumeroNova = $('#txtMudancaTipoAfiliacaoGLP').val();
        var AfiliacaoNomeNovo = $("#txtMudancaTipoAfiliacaoNome").val();

        ClearNotify();

        $.ajax({
            type: 'POST',
            url: rootURL + 'OrganismoAfiliado/Membro/ConfirmarMudancaTipoAfiliacao',
            data: { Id: @(Model.Id), AfiliacaoNumeroNova: AfiliacaoNumeroNova, AfiliacaoNomeNovo: AfiliacaoNomeNovo },
            beforeSend: function () {

            },
            success: function (data) {
                if (data.success) {
                    AbrirDetalhesMembro(@Model.Id);
                } else {
                    ShowNotify('warning', 'Mudança de Tipo de Afiliação', data.msg, 'pnlMembroDetalhe');
                }
            },
            error: function (xhr, st, str) {
                ShowError(MsgNaoFoiPossivelCompletarOperacao);
            },
            complete: function () {

            }
        });
    }

    $("#myModalFull").parent().find("div.ui-dialog-titlebar span").html("Visão Geral do Membro - @Model.Nome");
</script>