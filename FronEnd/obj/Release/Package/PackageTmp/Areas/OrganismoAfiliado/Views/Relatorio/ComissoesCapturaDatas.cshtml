@model List<SAL.MVC.Areas.OrganismoAfiliado.Models.RelatorioComissoesFiltroModel>
@if (Model != null && Model.Any())
{
    <table class="table table-striped table-hover" width="100%">
        <tbody>
            @foreach (var item in Model)
            {
                <tr style="cursor:pointer;" onclick="ComissoesImprimir('@item.Data.ToString("yyyy-MM-dd")')">
                    <td class="text-center">@item.Data.ToString("dd/MM/yyyy")</td>
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <text>Não existem movimentos para a referência selecionada.</text>
}