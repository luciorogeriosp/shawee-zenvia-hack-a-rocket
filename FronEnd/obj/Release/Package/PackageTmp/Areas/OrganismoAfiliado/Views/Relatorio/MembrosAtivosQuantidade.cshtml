    int intAnterior = 0;
    int intReintegrados = 0;
    int intDesligados = 0;
    int intNovos = 0;

    int intTotalAnterior = 0;
    int intTotalReintegrados = 0;
    int intTotalDesligados = 0;
    int intTotalNovos = 0;
    int intTotalAtual = 0;
