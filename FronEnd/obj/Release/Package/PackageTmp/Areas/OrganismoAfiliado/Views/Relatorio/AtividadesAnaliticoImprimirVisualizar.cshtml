@model List<SAL.MVC.Models.RelatorioModel<SAL.MVC.Areas.OrganismoAfiliado.Models.ListaPresencaAtividadeAbertaModel>>
<style>
    .page {
        padding-top: 33px;
    }

    .pageother {
        padding-top: -30px;
    }

    .titulo {
        font-size: 9pt;
        font-weight: bold;
    }

    .subtitulo {
        font-size: 7pt;
        font-weight: bold;
    }

    .texto {
        font-size: 9pt;
    }

    .textop {
        font-size: 9pt;
    }

    .texto-branco {
        font-size: 9pt;
        color: white;
    }

    .texto-left {
        text-align: left;
    }

    .texto-right {
        text-align: right;
    }

    .texto-center {
        text-align: center;
    }

    h1 {
        font-size: 12pt;
        text-align: center;
        font-weight: bold;
        padding: 0;
        margin: 0;
    }

    h2 {
        font-size: 10pt;
        text-align: center;
        font-weight: bold;
        padding: 0;
        margin: 0;
    }

    .valores {
        font-size: 7pt;
    }

    .espaco1 {
        font-size: 5pt;
    }

    .data {
        width: 60px;
        font-size: 7pt;
    }

    .frequencia {
        width: 50px;
    }

    .visitas {
        width: 50px;
    }

    .responsavel {
        width: 250px;
    }

    .descricao {
        width: 250px;
    }

    .tabela {
        border: 1px solid black;
    }

        .tabela th {
            border: 0.5px solid black;
        }

        .tabela td {
            border: 0.5px solid black;
        }
</style>
@for (int i = 0; i < Model.Count; i++)
{
    if (string.IsNullOrEmpty(Model[i].Dados.TipoLista) == false)
    {
        Html.RenderPartial("~/Areas/OrganismoAfiliado/Views/Atividade/ListasImprimirVisualizar_" + Model[i].Dados.TipoLista + ".cshtml", Model[i]);
        if (i + 1 < Model.Count)
        {
            <div style="page-break-before:always">&nbsp;</div>
        }
    }
}
