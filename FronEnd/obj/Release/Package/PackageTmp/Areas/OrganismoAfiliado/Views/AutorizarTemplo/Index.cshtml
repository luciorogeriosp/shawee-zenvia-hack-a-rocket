@model SAL.MVC.Areas.OrganismoAfiliado.Models.AutorizarTemploModel
@{
    Layout = null;
}
<ul id="myTab1" class="nav nav-tabs bordered">
    <li class="active">
        <a href="#pnlEntradaTemploNova" data-toggle="tab">Nova Autorização</a>
    </li>
    <li class="">
        <a href="#pnlEntradaTemploListar" onclick="ListaEntradaTemplo(false);" data-toggle="tab">Listar Autorizações</a>
    </li>
</ul>
<div class="tab-content padding-10">
    <div class="tab-pane fade active in" id="pnlEntradaTemploNova">
        @if (Model != null)
        {
            Html.RenderAction("Gerar", new { NumeroAfiliacao = Model.MembroAfiliacaoNumero, Nome = Model.MembroNome });
        }
    </div>
    <div class="tab-pane fade" id="pnlEntradaTemploListar"></div>
</div>
