<iframe id="iframeImprimir"
        name="iframeImprimir"
        title=""
        width="100%"
        height="300"
        frameborder="0"
        scrolling="yes"
        marginheight="0"
        marginwidth="0"
        src="@(Url.Action("MapaFinanceiroAnualImprimirVisualizar", "Relatorio"))"></iframe>
<div class="row">
    <div class="col-xs-3 col-sm-3 col-md-3 col-lg-3">
        <p>
            <label id="lblDataSelecionada"></label>
        </p>
    </div>
    <div class="col-xs-9 col-sm-9 col-md-9 col-lg-9" id="pnlMapaHistorico">

    </div>
</div>
<script>
    function resizeIframeImprimir(){
        $("#iframeImprimir").css("height", (parseInt($(window).height()) - 220) + "px");
    }

    resizeIframeImprimir();

    $(window).resize(function () {
        try {
            resizeIframeImprimir();
        } catch (e) {

        }
    });
</script>

