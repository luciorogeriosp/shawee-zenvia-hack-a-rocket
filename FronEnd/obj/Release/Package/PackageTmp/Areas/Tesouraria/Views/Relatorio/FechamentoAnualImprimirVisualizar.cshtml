                <td class="texto">@Model.Dados.SubTotalEntradas[i].Nome</td>
                <td class="espaco">&nbsp;</td>
                <td class="valores texto-@(Model.Dados.SubTotalEntradas[i].Valor > 0 ? "right" : "center")">@(Model.Dados.SubTotalEntradas[i].Valor > 0 ? string.Format("{0:N}", Model.Dados.SubTotalEntradas[i].Valor) : "-")</td>
                <td class="espaco1">&nbsp;</td>
                <td class="texto">&nbsp;</td>
            </tr>
        }
        <tr>
            <td class="texto">SOMA DAS ENTRADAS</td>
            <td class="espaco">&nbsp;</td>
            <td class="valores texto-right">@(string.Format("{0:N}", decEntradas))</td>
            <td class="espaco1">&nbsp;</td>
            <td class="texto">&nbsp;</td>
        </tr>
        <tr>
            <td class="texto" colspan="5">
                SALDO DO ANO ANTERIOR
            </td>
        </tr>
        <tr>
            <td class="texto">
                (Caixa, Bancos e Fundos)
            </td>
            <td class="espaco">&nbsp;</td>
            <td class="valores texto-right" valign="baseline">@(string.Format("{0:N}", Model.Dados.SaldoAnoAnterior))</td>
            <td class="espaco1">&nbsp;</td>
            <td class="texto">&nbsp;</td>
        </tr>
        <tr>
            <td colspan="5" class="espaco">&nbsp;</td>
        </tr>
        <tr>
            <td class="titulo">TOTAL DAS ENTRADAS</td>
            <td class="espaco">&nbsp;</td>
            <td class="texto">&nbsp;</td>
            <td class="espaco1">&nbsp;</td>
            <td class="valores titulo texto-right">@(string.Format("{0:N}", (Model.Dados.SaldoAnoAnterior + decEntradas)))</td>
        </tr>
        <tr>
            <td colspan="5" class="espaco">&nbsp;</td>
        </tr>
        <tr>
            <td colspan="5" class="texto">
                SAÍDAS:
            </td>
        </tr>
        @for (int i = 0; i < Model.Dados.Despesas.Count; i++)
        {
            decDespesas += Model.Dados.Despesas[i].Valor;
            decSaidas += Model.Dados.Despesas[i].Valor;
            <tr>
                <td class="texto">@Model.Dados.Despesas[i].Nome</td>
