@model SAL.MVC.Models.RelatorioModel<SAL.MVC.Areas.Tesouraria.Models.RelatorioResumoFinanceiroAnualModel>
@{
    decimal decRecebimento = 0;
    decimal decSubTotalEntradas = 0;
    decimal decEntradas = 0;

    decimal decDespesas = 0;
    decimal decSubTotalSaidas = 0;
    decimal decSaidas = 0;

    decimal decSaldoGarantias = 0;
    decSaldoGarantias +=
        (Model.Dados.SaldoCaixa.HasValue ? Model.Dados.SaldoCaixa.Value : 0)
        +
        (Model.Dados.SaldoContaCorrente.HasValue ? Model.Dados.SaldoContaCorrente.Value : 0)
        +
        (Model.Dados.SaldoContaPoupanca.HasValue ? Model.Dados.SaldoContaPoupanca.Value : 0)
        +
        (Model.Dados.SaldoAplicacao.HasValue ? Model.Dados.SaldoAplicacao.Value : 0)
        ;

    DateTime dtAtual = SAL.Repository.Negocio.UtilNegocio.ObterDataHoraAtual();
    string dtAtualTexto = dtAtual.Day + " de " + SAL.MVC.Helpers.Constantes.DescricaoMes(dtAtual.Month) + " de " + dtAtual.Year;
}
<style>
    .page {
        padding-top: 65px;
    }

    .pageother {
        padding-top: 45px;
    }

    .titulo {
        font-size: 9pt;
        font-weight: bold;
    }

    .texto {
        font-size: 9pt;
    }

    .textop {
        font-size: 9pt;
    }

    .texto-branco {
        font-size: 9pt;
        color: white;
    }

    .texto-left {
        text-align: left;
    }

    .texto-right {
        text-align: right;
    }

    .texto-center {
        text-align: center;
    }

    h1 {
        font-size: 12pt;
        text-align: center;
        font-weight: bold;
        padding: 0;
        margin: 0;
    }

    h2 {
        font-size: 12pt;
        text-align: center;
        font-weight: bold;
        padding: 0;
        margin: 0;
    }

    .valores {
        width: 100px;
        font-size: 9pt;
        border-bottom-color: #000000;
        border-bottom-width: 1px;
        border-bottom-style: solid;
    }

    .espaco1 {
        width: 100px;
        font-size: 5pt;
    }
</style>
<div class="page">
    <h1>ORGANISMO AFILIADO: @Model.ReportTitle</h1>
    <h2>DECLARAÇÃO I</h2>
    <table class="tabela" cellpadding="1" cellspacing="0" width="100%">
        <tr>
            <td colspan="5" class="texto">&nbsp;</td>
        </tr>
        <tr>
            <td colspan="5" class="titulo">
                Receita & Despesa
            </td>
        </tr>
        <tr>
            <td colspan="5" class="texto">
                (Correspondente à soma dos relatórios mensais do ano civil) ANO: @Model.Dados.Ano
            </td>
        </tr>
        <tr>
            <td colspan="5" class="espaco">&nbsp;</td>
        </tr>
        <tr>
            <td colspan="5" class="texto">
                ENTRADAS:
            </td>
        </tr>
        @for (int i = 0; i < Model.Dados.Recebimento.Count; i++)
        {
            decRecebimento += Model.Dados.Recebimento[i].Valor;
            decEntradas += Model.Dados.Recebimento[i].Valor;
            <tr>
                <td class="texto">@Model.Dados.Recebimento[i].Nome</td>
                <td class="espaco">&nbsp;</td>
                <td class="valores texto-@(Model.Dados.Recebimento[i].Valor > 0 ? "right" : "center")">@(Model.Dados.Recebimento[i].Valor > 0 ? string.Format("{0:N}", Model.Dados.Recebimento[i].Valor) : "-")</td>
                <td class="espaco1">&nbsp;</td>
                <td class="texto">&nbsp;</td>
            </tr>
        }
        <tr>
            <td class="texto">SUB-TOTAL</td>
            <td class="espaco">&nbsp;</td>
            <td class="valores texto-right">@(string.Format("{0:N}", decRecebimento))</td>
            <td class="espaco1">&nbsp;</td>
            <td class="texto">&nbsp;</td>
        </tr>
        <tr>
            <td colspan="5" class="espaco">&nbsp;</td>
        </tr>
        @for (int i = 0; i < Model.Dados.SubTotalEntradas.Count; i++)
        {
            decSubTotalEntradas += Model.Dados.SubTotalEntradas[i].Valor;
            decEntradas += Model.Dados.SubTotalEntradas[i].Valor;
