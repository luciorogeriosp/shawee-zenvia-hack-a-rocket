                <td class="valores texto-@(Model.Dados.Despesas[i].Valor > 0 ? "right" : "center")">@(Model.Dados.Despesas[i].Valor > 0 ? string.Format("{0:N}", Model.Dados.Despesas[i].Valor) : "-")</td>
                <td class="espaco1">&nbsp;</td>
                <td class="texto">&nbsp;</td>
            </tr>
        }
        <tr>
            <td class="texto">SUB-TOTAL</td>
            <td class="espaco">&nbsp;</td>
            <td class="valores texto-right">@(string.Format("{0:N}", decSaidas))</td>
            <td class="espaco1">&nbsp;</td>
            <td class="texto">&nbsp;</td>
        </tr>
        <tr>
            <td colspan="5" class="espaco">&nbsp;</td>
        </tr>
        @for (int i = 0; i < Model.Dados.SubTotalSaidas.Count; i++)
        {
            decSubTotalSaidas += Model.Dados.SubTotalSaidas[i].Valor;
            decSaidas += Model.Dados.SubTotalSaidas[i].Valor;
