                <td class="texto">@Model.Dados.SubTotalSaidas[i].Nome</td>
                <td class="espaco">&nbsp;</td>
                <td class="valores texto-@(Model.Dados.SubTotalSaidas[i].Valor > 0 ? "right" : "center")">@(Model.Dados.SubTotalSaidas[i].Valor > 0 ? string.Format("{0:N}", Model.Dados.SubTotalSaidas[i].Valor) : "-")</td>
                <td class="espaco1">&nbsp;</td>
                <td class="texto">&nbsp;</td>
            </tr>
        }
        <tr>
            <td colspan="5" class="espaco">&nbsp;</td>
        </tr>
        <tr>
            <td class="titulo">TOTAL DAS SAÍDAS</td>
            <td class="espaco">&nbsp;</td>
            <td class="texto">&nbsp;</td>
            <td class="espaco1">&nbsp;</td>
            <td class="valores titulo texto-right">@string.Format("{0:N}", decSaidas)</td>
        </tr>
        <tr>
            <td colspan="5" class="espaco">&nbsp;</td>
        </tr>
        <tr>
            <td>
                <span class="titulo">SALDO PARA O ANO PRÓXIMO<br /></span>
                <span class="texto">(Caixa, Bancos e Fundos)</span>
            </td>
            <td class="espaco">&nbsp;</td>
            <td class="texto">&nbsp;</td>
            <td class="espaco1">&nbsp;</td>
            <td class="valores titulo texto-right">@(string.Format("{0:N}", Model.Dados.SaldoAnoAnterior + decEntradas - decSaidas))</td>
        </tr>
