<iframe id="iframeImprimir"
        name="iframeImprimir"
        title=""
        width="100%"
        height="300"
        frameborder="0"
        scrolling="no"
        marginheight="0"
        marginwidth="0"
        src="@(Url.Action("ListagemPagamentoImprimirVisualizar", "Relatorio"))"></iframe>
<script>
    function resizeIframeImprimir(){
        $("#iframeImprimir").css("height", (parseInt($(window).height()) - 220) + "px");
    }

    resizeIframeImprimir();

    $(window).resize(function () {
        try {
            resizeIframeImprimir();
        } catch (e) {

        }
    });
</script>

