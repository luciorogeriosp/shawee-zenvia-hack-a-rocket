@model List<SAL.MVC.Areas.Recebedoria.Models.RelatorioDiarioFiltro>
@if (Model != null && Model.Any())
{
    <script>
        $("#txtDataInicial").val('@(Model[Model.Count - 1].Data.ToString("yyyy-MM-dd"))');
        $("#txtDataFinal").val('@(Model[0].Data.ToString("yyyy-MM-dd"))');
    </script>
    <table class="table table-striped table-hover" width="100%">
        <tbody>
            @foreach (var item in Model)
            {
                <tr style="cursor:pointer;" onclick="FiltroDataEspecifica('@item.Data.ToString("yyyy-MM-dd")')">
                    <td class="text-center">@item.Data.ToString("dd/MM/yyyy")</td>
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <text>Não existem movimentos para a referência selecionada.</text>
}